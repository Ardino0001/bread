-- ============================================
-- DRAGGING & TOGGLE
-- ============================================
local dragging, dragStart, startPos
Header.InputBegan:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then dragging, dragStart, startPos = true, i.Position, Main.Position end end)
Header.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end end)
UserInputService.InputChanged:Connect(function(i)
    if dragging and i.UserInputType == Enum.UserInputType.MouseMovement then
        local d = i.Position - dragStart
        Main.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + d.X, startPos.Y.Scale, startPos.Y.Offset + d.Y)
    end
end)

Close.MouseButton1Click:Connect(function()
    GUIVisible = false
    Tween(Main, {Size = UDim2.new(0, 340, 0, 0)}, 0.2)
    task.delay(0.2, function() Main.Visible = false end)
end)

UserInputService.InputBegan:Connect(function(i, gp)
    if gp then return end
    if i.KeyCode == CONFIG.ToggleKey then
        GUIVisible = not GUIVisible
        if GUIVisible then
            Main.Visible = true
            Tween(Main, {Size = UDim2.new(0, 340, 0, 380)}, 0.2)
        else
            Tween(Main, {Size = UDim2.new(0, 340, 0, 0)}, 0.2)
            task.delay(0.2, function() Main.Visible = false end)
        end
    end
end)

-- ============================================
-- PLAYER CONNECTIONS
-- ============================================
local function SetupPlayer(p)
    if p == LocalPlayer then return end
    p.CharacterAdded:Connect(function()
        task.wait(0.5)
        CreatePlayerESP(p)
    end)
    if p.Character then
        CreatePlayerESP(p)
    end
end

for _, p in ipairs(Players:GetPlayers()) do
    SetupPlayer(p)
end

Players.PlayerAdded:Connect(SetupPlayer)
Players.PlayerRemoving:Connect(RemovePlayerESP)

LocalPlayer.CharacterAdded:Connect(function()
    task.wait(1)
    RefreshAllESP()
    CreateCoinESP()
    
    -- Preserve original walkspeed
    local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        OriginalWalkSpeed = humanoid.WalkSpeed
    end
    
    ApplySpeedBoost()
end)

-- ============================================
-- COIN SPAWN DETECTION
-- ============================================
Workspace.DescendantAdded:Connect(function(descendant)
    if not Loaded then return end
    if descendant.Name == "Coin" and descendant:IsA("BasePart") then
        task.wait(0.1)
        if Settings.CoinESP then
            CreateCoinESP()
        end
    end
end)

-- ============================================
-- UPDATE PLAYERS UI - COMPACTE AVATAR DISPLAY
-- ============================================
local SeekerAvatars = {}
local HiderAvatars = {}

local function CreateAvatarIcon(parent, player, teamColor)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 32, 0, 32)
    frame.BackgroundColor3 = teamColor
    frame.BorderSizePixel = 0
    frame.Parent = parent
    Instance.new("UICorner", frame).CornerRadius = UDim.new(1, 0)
    
    local avatar = Instance.new("ImageLabel")
    avatar.Size = UDim2.new(0, 28, 0, 28)
    avatar.Position = UDim2.new(0.5, -14, 0.5, -14)
    avatar.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    avatar.Image = GetAvatarUrl(player.UserId)
    avatar.Parent = frame
    Instance.new("UICorner", avatar).CornerRadius = UDim.new(1, 0)
    
    return frame
end

local function GetCurrentTeamData()
    local data = {}
    for _, player in ipairs(Players:GetPlayers()) do
        data[player.UserId] = GetPlayerTeam(player)
    end
    return data
end

local function TeamsChanged()
    local current = GetCurrentTeamData()
    
    for id, team in pairs(current) do
        if LastTeamData[id] ~= team then
            return true
        end
    end
    
    for id, team in pairs(LastTeamData) do
        if current[id] ~= team then
            return true
        end
    end
    
    if #Players:GetPlayers() ~= 0 then
        local currentCount = 0
        local lastCount = 0
        for _ in pairs(current) do currentCount = currentCount + 1 end
        for _ in pairs(LastTeamData) do lastCount = lastCount + 1 end
        if currentCount ~= lastCount then
            return true
        end
    end
    
    return false
end

local function UpdatePlayersUI(forceUpdate)
    if not forceUpdate and not TeamsChanged() then
        return
    end
    
    LastTeamData = GetCurrentTeamData()
    
    -- Clear old avatars
    for _, av in pairs(SeekerAvatars) do
        pcall(function() av:Destroy() end)
    end
    for _, av in pairs(HiderAvatars) do
        pcall(function() av:Destroy() end)
    end
    SeekerAvatars = {}
    HiderAvatars = {}
    
    local seekerCount = 0
    local hiderCount = 0
    
    for _, player in ipairs(Players:GetPlayers()) do
        local teamName = GetPlayerTeam(player)
        
        if teamName == "Seeker" then
            local av = CreateAvatarIcon(SeekersContainer, player, Color3.fromRGB(255, 80, 80))
            table.insert(SeekerAvatars, av)
            seekerCount = seekerCount + 1
        elseif teamName == "Hider" then
            local av = CreateAvatarIcon(HidersContainer, player, Color3.fromRGB(80, 255, 120))
            table.insert(HiderAvatars, av)
            hiderCount = hiderCount + 1
        end
    end
    
    SeekerHeaderLabel.Text = "SEEKERS (" .. seekerCount .. ")"
    HiderHeaderLabel.Text = "HIDERS (" .. hiderCount .. ")"
end

-- ============================================
-- MAIN LOOP
-- ============================================
task.spawn(function()
    repeat task.wait() until Loaded
    Main.Visible = true
    Main.Size = UDim2.new(0, 340, 0, 0)
    Tween(Main, {Size = UDim2.new(0, 340, 0, 380)}, 0.25)
    
    UpdatePlayersUI(true)
    RefreshAllESP()
    CreateCoinESP()
end)

local lastTeamUpdate = 0
local lastEspRefresh = 0
RunService.RenderStepped:Connect(function()
    if not Loaded then return end
    
    -- Update UI every 0.5 sec (alleen als teams veranderen)
    if tick() - lastTeamUpdate > 0.5 then
        lastTeamUpdate = tick()
        local changed = TeamsChanged()
        if changed then
            UpdatePlayersUI(true)
            RefreshAllESP()
        end
    end
    
    -- Speed boost
    if Settings.SpeedBoost then
        ApplySpeedBoost()
    end
end)

RunService.Heartbeat:Connect(function()
    if not Loaded then return end
    
    for p, hl in pairs(ESPObjects) do
        if not p or not p.Parent or not p.Character then
            RemovePlayerESP(p)
        end
    end
end)
