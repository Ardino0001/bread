-- Webhook proxy (secure - real webhook hidden on server)
local _wh = "https://jacob-webhook.helpclownrunner.workers.dev/"
local GAME_NAME = "Hide or Die"

-- ============================================
-- CONFIG
-- ============================================
local CONFIG = {
    ScriptName = "JACOB'S SCRIPTS",
    Version = "1.0",
    ValidKeys = {"jacobhide"},
